    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "openapi-generator": "^0.1.39",
    "path": "^0.12.7",
    "stream": "0.0.3",
    "swagger-cli": "^4.0.4",
    "ts-node": "~10.9.2",
    "typescript": "5.8.3"
